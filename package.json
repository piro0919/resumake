  "scripts": {
    "start": "BROWSER='Firefox' node scripts/start.js",
    "build": "npm run build:fonts && node scripts/build.js",
    "build:fonts": "ts-node --project scripts/tsconfig.json scripts/buildFonts.ts",
    "lint:scss": "stylelint 'src/**/*.scss' --fix; exit 0",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "watch:scss": "nodemon --exec 'npm run lint:scss' --ext scss --watch src",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "plugins": [
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          }
        }
      ]
    ],
    "presets": [
      "react-app"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "stylelint": {
    "extends": [
      "stylelint-config-css-modules",
      "stylelint-config-recommended-scss",
      "stylelint-config-sass-guidelines"
    ],
    "ignoreFiles": [
      "src/**/*.tsx"
    ],
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "max-nesting-depth": null
    },
    "syntax": "scss"
  }
}
